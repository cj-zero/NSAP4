<body>
    <div id="app">
        <header class="title font-roboto--medium ">
            <div>
                校准证书
            </div>
        </header>
        <div style="text-align: center; margin-top: -10px; font-size: 18pt; font-family: 'Times New Roman'; font-weight: bold; ">
            Calibration Certificate
        </div>
        <table class="table table--border-top">
            <tbody>
                <tr>
                    <td style="width: 150px; ">
                        <div class="text-15pt">
                            证 书 编 号：
                        </div>
                        <div class="text-en-12pt">
                            Certificate No.
                        </div>
                    </td>
                    <td>
                        <div style="font-size:13pt;font-family:'Times New Roman'">
                            <span>@Model.Data.CalibrationCertificate.CertificatenNumber</span>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="text-15pt">
                            委 托 单 位：
                        </div>
                        <div class="text-en-12pt">
                            Client
                        </div>
                    </td>
                    <td>
                        <div style="font-size:13pt;font-family:FangSong;">
                            @Model.Data.CalibrationCertificate.EntrustedUnit
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="text-15pt">
                            地 址：
                        </div>
                        <div class="text-en-12pt">
                            Address
                        </div>
                    </td>
                    <td>
                        <div style="font-size: 13pt; font-family: FangSong">
                            @Model.Data.CalibrationCertificate.EntrustedUnitAdress
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="text-15pt">
                            仪 器 名 称：
                        </div>
                        <div class="text-en-12pt">
                            Description
                        </div>
                    </td>
                    <td>
                        <div style="font-size: 13pt; font-family: FangSong">
                            电池充放电测试仪
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="text-15pt">
                            制 造 厂：
                        </div>
                        <div class="text-en-12pt">
                            Manufacturer
                        </div>
                    </td>
                    <td>
                        <div style="font-size:13pt;font-family:Cambria">
                            @Model.Data.CalibrationCertificate.TesterMake
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="text-15pt">
                            型 号 规 格：
                        </div>
                        <div class="text-en-12pt">
                            Model/Type
                        </div>
                    </td>
                    <td>
                        <div style="font-size: 13pt; font-family: FangSong">
                            @Model.Data.CalibrationCertificate.TesterModel
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="text-15pt">
                            出 厂 编 号：
                        </div>
                        <div class="text-en-12pt">
                            Serial  No.
                        </div>
                    </td>
                    <td>
                        <div style="font-size: 13pt; font-family: FangSong">
                            @Model.Data.CalibrationCertificate.TesterSn
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="text-15pt">
                            资 产 编 号：
                        </div>
                        <div class="text-en-12pt">
                            Asset  No.
                        </div>
                    </td>
                    <td>
                        <div style="font-size: 13pt; font-family: FangSong">
                            @Model.Data.CalibrationCertificate.AssetNo
                        </div>
                    </td>
                </tr>
