@using OpenAuth.App.Material.Response
<!doctype html>
<html>

<head>
    <meta charset="utf-8">
    <title></title>
    <style type="text/css">
        * {
            padding: 0;
            margin: 0;
        }

        body {
            width: 1000px;
            margin: 0 auto;
            margin-top: 10px;
            font-size: 13.5px;
            font-family: "微软雅黑";
            color: black;
        }

        table {
            border-collapse: collapse;
        }

        #tablebor {
            margin: 0 auto;
            border-collapse: collapse;
        }

        .TH td {
            border-style: solid;
            border-width: 1px;
        }

        #divtitle {
            margin: 0 auto;
        }

        hr {
            margin-top: 80px;
            height: 0px;
            border-top: 1px solid #808A87;
            border-right: 0px;
            border-bottom: 0px;
            border-left: 0px;
        }

        #divcode {
            height: 50px;
            width: 1000px;
            margin: 0 auto;
            border: 1px solid #808A87;
        }
    </style>
</head>

<body style="position:relative;top:270px">
    <table style="width:1000px">
        @{int number = 0;}
        @foreach (var item in Model.Data)
        {
            <tr>
                <td style="width: 30px;" align="center">@{++number;} @number </td>
                <td style="width: 150px;" align="left">&nbsp; @item.MaterialCode</td>
                <td style="width: 500px; align:left;">@item.MaterialDescription</td>
                <td style="width: 50px;" align="right"> @item.Count&nbsp;</td>
                <td style="width: 50px; " align="left">&nbsp;@item.Unit</td>
                <td style="width: 50px; " align="right">&nbsp;@item.Good</td>
                <td style="width: 50px; " align="right">&nbsp;@item.Second</td>
                @*<td style="width: 50px; " align="left">&nbsp;@item.WhsCode</td>*@
            </tr>
        }

    </table>
    @*<div style="border: 1px solid #7F7F7F; width:1000px;height:200px; margin-top:20px;">
        <span style="font-weight:900;font-size:18px;">&nbsp;产品编号/箱号：</span>
    </div>*@
</body>

</html>
