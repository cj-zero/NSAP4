<body>
    <div class="productList" style="width: 1002px; font-weight: 400; ">
        @{
            int number = 0;
        }
        @foreach (var item in Model.Data.ReimburseCosts)
        {

            <div>
                <span class="index_">
                    @{
                        ++number;
                    } @number
                </span>
                <span class="wlCode_">@item.ItemCode</span>
                <span class="wlDsc_">@item.Dscription</span>
                <span class="num_">@item.Quantity</span>
                <span class="type_"> @item.unitMsr</span>
                <span class="onePrice_">@item.Price</span>
                <span class="prvePrice_"> @item.Money</span>
            </div>



        }
      
    </div>

